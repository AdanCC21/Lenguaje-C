#include <stdio.h>

int main()
{
    char *ptr[5];
    int i;
    for(i=0;i<3;i++)
    {
        printf("ingrese el nombre numero %d\n",i+1);
        char ptr[50];
        scanf("%s",ptr);
    }

    for(i=0;i<3;i++)
    {
        printf("Sus nombres son %s\n",*ptr[i]);
    }
}